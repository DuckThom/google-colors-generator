{
    "name": "luna/google-color-scss-generator",
    "description": "Generate an up-to-date file with every color from the Google Color scheme located at https://material.google.com/style/color.html",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "symfony/dom-crawler": "^3.0",
        "illuminate/filesystem": "^5.2",
        "symfony/css-selector": "^3.0",
        "symfony/console": "^3.0",
        "phpunit/phpunit": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Wiringa",
            "email": "thomas.wiringa@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "app/Helpers/functions.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
